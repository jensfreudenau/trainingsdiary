= form_for @workout,  :html => { :id => 'training', :class => :form, :multipart => true} do |f|
  - if @workout.errors.any?
    #error_explanation
      %h2= "#{pluralize(@workout.errors.count, "error")} prohibited this workout from being saved:"
      %ul
        - @workout.errors.full_messages.each do |msg|
          %li= msg
  = f.file_field :file_name, :class => 'file_field'
  .actions
    = f.submit 'Save'
